<resources>
    <!-- Base application theme. -->
    <style name="Theme.Reuseit" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#299E77</item>
        <item name="colorPrimaryVariant">#20805F</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#1F805C</item>
        <item name="colorSecondaryVariant">#166247</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily" >@font/poppins</item>
    </style>

    <style name="DisableBackground" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@android:color/transparent</item>
    </style>
</resources>